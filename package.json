{
  "name": "firestorable",
  "version": "1.0.0",
  "description": "Mobx observable wrapper for firebase firestore database.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "emulator": "firebase emulators:start --only firestore",
    "emulator:exec": "firebase emulators:exec --only firestore",
    "test": "jest",
    "test:emulator": "firebase emulators:exec --only firestore jest",
    "test:emulator:coverage": "firebase emulators:exec --only firestore \"jest --coverage\"",
    "coverage": "start coverage\\lcov-report\\index.html",
    "build": "rollup -c",
    "prepublishOnly": "npm test:emulator",
    "prepare": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thdk/firestorable.git"
  },
  "keywords": [
    "firestore",
    "mobx",
    "firebase",
    "observable",
    "realtime",
    "thdk"
  ],
  "author": "Thomas Dekiere (t.dekiere@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thdk/firestorable/issues"
  },
  "homepage": "https://github.com/thdk/firestorable#readme",
  "peerDependencies": {
    "mobx": ">= 5"
  },
  "dependencies": {},
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.0.7",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@testing-library/dom": "^7.24.5",
    "@types/jest": "^24.0.23",
    "firebase": "^7.23.0",
    "firebase-admin": "^9.2.0",
    "jest": "^25.3.0",
    "mobx": "^5.15.0",
    "rollup": "^2.6.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.6.0"
  }
}